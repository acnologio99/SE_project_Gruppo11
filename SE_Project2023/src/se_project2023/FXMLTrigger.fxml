   <children>
      <ListView fx:id="triggerListView" layoutX="31.0" layoutY="50.0" prefHeight="331.0" prefWidth="200.0" />
      <JFXTimePicker fx:id="timePicker" defaultColor="#6c0094" layoutX="300.0" layoutY="79.0" onAction="#timePick" prefHeight="31.0" prefWidth="233.0" />
      <Button fx:id="Done" layoutX="430.0" layoutY="350.0" mnemonicParsing="false" onAction="#doneTrigger" text="Done!" />
      <Button fx:id="Cancel" layoutX="521.0" layoutY="350.0" mnemonicParsing="false" onAction="#cancelTrigger" text="Cancel" />
      <Button fx:id="fileTriggerButton" layoutX="300.0" layoutY="169.0" mnemonicParsing="false" onAction="#selectFolder" prefHeight="31.0" prefWidth="233.0" text="Scegli una cartella..." />
   </children>
</AnchorPane>
